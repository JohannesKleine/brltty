#!/bin/bash
. /brltty/current/libexec/canute-prologue.sh

processExtraProgramParameters() {
   declare -g -r clientParameters=("${testName}" "${@}")
}

addTestModeOption
addProgramParameter test testName "the name of the test to be performed - specify -- -help for the usage summary"
optionalProgramParameters argument "the test's arguments"
parseProgramArguments "${@}"

executeHostCommand exec canute-java org.a11y.brlapi.MainProgram "${clientParameters[@]}"
exit 0
