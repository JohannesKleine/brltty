#!/bin/bash
. /brltty/current/libexec/canute-prologue.sh

showProgramUsagePurpose() {
cat <<END_OF_PROGRAM_USAGE_PURPOSE
Run a BrlAPI client within a prepared environment.
Java clients aren't supported (use canute-java instead).
END_OF_PROGRAM_USAGE_PURPOSE
}

processExtraProgramParameters() {
   declare -g -r clientParameters=("${clientCommand}" "${@}")
}

addProgramParameter command clientCommand "the na,e of, or the relative or absolute path to, the command"
optionalProgramParameters argument "the command's arguments"
parseProgramArguments "${@}"

export PYTHONPATH="${apiLinksDirectory}/python"
export TCLLIBPATH="${apiLinksDirectory}/tcl"

canuteSetSession
executeHostCommand exec "${clientParameters[@]}"
exit 0
