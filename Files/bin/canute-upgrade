#!/bin/bash
. /brltty/current/libexec/canute-prologue.sh

showProgramUsagePurpose() {
cat <<END_OF_PROGRAM_USAGE_PURPOSE
Upgrade the Raspbian Canute Controller software.
END_OF_PROGRAM_USAGE_PURPOSE
}

parseProgramArguments "${@}"

set -e
needTemporaryDirectory
executeHostCommand cd "${temporaryDirectory}"

executeHostCommand cp "${installTree}/libexec/canute-install" .
executeHostCommand ./canute-install
exit 0
